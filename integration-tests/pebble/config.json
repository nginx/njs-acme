{
    "pebble": {
      "listenAddress": "0.0.0.0:443",
      "managementListenAddress": "0.0.0.0:15000",
      "certificate": "/etc/pebble/cert.pem",
      "privateKey": "/etc/pebble/key.pem",
      "httpPort": 8000,
      "tlsPort": 5001,
      "ocspResponderURL": "",
      "externalAccountBindingRequired": false,
      "domainBlocklist": ["blocked-domain.example"],
      "retryAfter": {
          "authz": 30,
          "order": 50
      }
    }
  }
